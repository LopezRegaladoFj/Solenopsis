/**
 * IO utility class.
 */
IOUtils() {
    /**
     * Attempt to close an InputStream.
     *
     * @param is the input stream to close.
     */
    void close(InputStream is) {
        try {
            is.close();
        } catch (IOException ioException) {
            System.err.println("Trouble closing input stream!");
        }
    }

    return this;
}